{
    "apiVersion": "v1",
    "kind": "List",
    "metadata": {
        "name": "network-policy-form-test",
        "namespace": "test-ndsyf",
        "labels": {
            "automatedTestName": "test-ndsyf"
        }
    },
    "items": [
        {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "test-rc"
                },
                "name": "test-rc"
            },
            "spec": {
                "replicas": 2,
                "template": {
                    "metadata": {
                        "labels": {
                            "name": "test-pods"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "quay.io/openshifttest/hello-sdn:latest",
                                "name": "test-pod",
                                "imagePullPolicy": "IfNotPresent",
                                "resources": {
                                    "limits": {
                                        "memory": "340Mi"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "labels": {
                    "name": "test-rc2"
                },
                "name": "test-rc2"
            },
            "spec": {
                "replicas": 2,
                "template": {
                    "metadata": {
                        "labels": {
                            "name": "test-pods2"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "image": "quay.io/openshifttest/hello-sdn:latest",
                                "name": "test-pod2",
                                "imagePullPolicy": "IfNotPresent",
                                "resources": {
                                    "limits": {
                                        "memory": "340Mi"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "test-service"
                },
                "name": "test-service"
            },
            "spec": {
                "ports": [
                    {
                        "name": "http",
                        "port": 27017,
                        "protocol": "TCP",
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "name": "test-pods"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "labels": {
                    "name": "test-service2"
                },
                "name": "test-service2"
            },
            "spec": {
                "ports": [
                    {
                        "name": "http",
                        "port": 27017,
                        "protocol": "TCP",
                        "targetPort": 8888
                    }
                ],
                "selector": {
                    "name": "test-pods2"
                }
            }
        }
    ]
}